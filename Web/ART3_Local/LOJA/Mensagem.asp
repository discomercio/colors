'	MENSAGEM PARA EXIBIÇÃO
	dim mensagem, url_back
	mensagem = Session(SESSION_CLIPBOARD)

	url_back = Trim(request("url_back"))
	if url_back = "" then 
		url_back = "resumo.asp" & "?" & MontaCampoQueryStringSessionCtrlInfo(Session("SessionCtrlInfo"))
	else
		if Instr(url_back, "?") > 0 then
			url_back = url_back & "&" & MontaCampoQueryStringSessionCtrlInfo(Session("SessionCtrlInfo"))
		else
			url_back = url_back & "?" & MontaCampoQueryStringSessionCtrlInfo(Session("SessionCtrlInfo"))
